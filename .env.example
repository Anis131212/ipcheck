# IPQualityScore API Key (Required for Risk Score)
# Get it from: https://www.ipqualityscore.com/user/dashboard
IPQS_KEY=

# AbuseIPDB API Key (Optional, for additional threat data)
# Get it from: https://www.abuseipdb.com/account/api
ABUSEIPDB_KEY=

# Redis Configuration (Default is usually fine for Docker)
REDIS_HOST=redis

# LLM Configuration (Optional, for advanced IP analysis)
# Compatible with OpenAI API format (e.g., OpenAI, DeepSeek, SiliconFlow)
LLM_BASE_URL=https://api.openai.com/v1
LLM_API_KEY=
LLM_MODEL=gpt-3.5-turbo

# New Data Sources (Optional)
# IP2Location.io Key (https://www.ip2location.io/)
IP2LOCATION_KEY=

# Scamalytics Key (https://scamalytics.com/ip/api)
# Format: username:key (if required by implementation) or just separate fields
SCAMALYTICS_USERNAME=
SCAMALYTICS_KEY=

# IPData Key (https://ipdata.co/)
IPDATA_KEY=
